{
    "plugins": [
      "js-sequence-diagram-full@>=0.3.1",
      "prism",
      "-highlight",
      "anchors",
      "splitter",
      "github",
      "anchor-navigation-ex",
      "expandable-chapters-small",
      "pageview-count",
      "language-picker",
      "hide-element"
    ],
    "pluginsConfig": {
      "prism": {
        "css": [
          "prismjs/themes/prism-okaidia.css"
        ],
        "lang": {
          "java": "java"
        },
        "ignore": [
          "sequence"
        ]
      },
      "github": {
        "url": "https://github.com/TuyaInc/tuyasmart_panel_android_sdk"
      },
      "anchor-navigation-ex": {
        "showGoTop": false
      },
      "hide-element": {
              "elements": [".gitbook-link"]
      }
    }
  }